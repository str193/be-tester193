from selenium import webdriver
driver = webdriver.Chrome(executable_path='C:/chromedriver.exe')
driver.maximize_window()
driver.implicitly_wait(5)
driver.get("https://demo.automationtesting.in/WebTable.html")
mo = driver.find_element_by_link_text('More')
mo.click()
lo = driver.find_element_by_link_text('File Upload')
lo.click()
file1 = (r'C:\Users\Str193\Desktop\Ivan S\patriot.jpg')
upload1 = driver.find_element_by_css_selector("input#input-4")
upload1.send_keys(file1)
rem = driver.find_element_by_css_selector(".btn.btn-default.fileinput-remove.fileinput-remove-button")
rem.click()
file2 = (r'C:\Users\Str193\Desktop\less12.txt')
upload2 = driver.find_element_by_css_selector("input#input-4")
upload2.send_keys(file2)
driver.quit()
